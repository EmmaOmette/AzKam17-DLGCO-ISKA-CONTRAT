{% extends 'base.html.twig' %}

{% block title %}Obligation{% endblock %}

{% block body %}
    <h1>Obligation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ obligation.id }}</td>
            </tr>
            <tr>
                <th>SourceDisposition</th>
                <td>{{ obligation.sourceDisposition }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ obligation.reference }}</td>
            </tr>
            <tr>
                <th>PointsAbordes</th>
                <td>{{ obligation.pointsAbordes }}</td>
            </tr>
            <tr>
                <th>Obligation</th>
                <td>{{ obligation.obligation }}</td>
            </tr>
            <tr>
                <th>Sanctions</th>
                <td>{{ obligation.sanctions }}</td>
            </tr>
            <tr>
                <th>Prevues</th>
                <td>{{ obligation.prevues }}</td>
            </tr>
            <tr>
                <th>Impact</th>
                <td>{{ obligation.impact }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_gestion_obligation_obligation_index') }}">back to list</a>

    <a href="{{ path('app_gestion_obligation_obligation_edit', {'id': obligation.id}) }}">edit</a>

    {{ include('gestion_obligation/obligation/_delete_form.html.twig') }}
{% endblock %}
